<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ninobackground"
    tools:context=".ActividadResta">

    <ImageView
        android:id="@+id/btnNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="31dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow17"
        app:srcCompat="@drawable/compartir" />

    <TableRow
        android:id="@+id/tableRow12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toTopOf="@+id/tableRow15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/btnVolver2"
            android:layout_width="77dp"
            android:layout_height="74dp"
            app:srcCompat="@mipmap/volver_foreground" />

        <Space
            android:layout_width="63dp"
            android:layout_height="match_parent" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="101dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/menos" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow15"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="52dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="47dp"
        app:layout_constraintBottom_toTopOf="@+id/tableRow16"
        app:layout_constraintEnd_toEndOf="@+id/tableRow16"
        app:layout_constraintStart_toStartOf="@+id/tableRow17"
        app:layout_constraintTop_toBottomOf="@+id/tableRow12">

        <RatingBar
            android:id="@+id/ratingBar2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow16"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="196dp"
        app:layout_constraintBottom_toTopOf="@+id/tableRow17"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow15">

        <TextView
            android:id="@+id/tvNum1"
            android:layout_width="81dp"
            android:layout_height="56dp"
            android:gravity="center"
            android:text="TextView" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="21dp"
            android:layout_height="61dp"
            app:srcCompat="@drawable/resta1" />

        <TextView
            android:id="@+id/tvNum2"
            android:layout_width="78dp"
            android:layout_height="60dp"
            android:gravity="center"
            android:text="TextView" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="22dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/igual" />

        <Space
            android:layout_width="17dp"
            android:layout_height="58dp" />

        <EditText
            android:id="@+id/etRespuesta"
            android:layout_width="133dp"
            android:layout_height="54dp"
            android:background="@drawable/rounded_edittext"
            android:ems="10"
            android:gravity="center"
            android:inputType="number" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow17"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="22dp"
        app:layout_constraintBottom_toTopOf="@+id/btnNext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow16">

        <Button
            android:id="@+id/btnOp1"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:text="Button" />

        <Space
            android:layout_width="25dp"
            android:layout_height="73dp" />

        <Button
            android:id="@+id/btnOp2"
            android:layout_width="wrap_content"
            android:layout_height="71dp"
            android:text="Button" />

        <Space
            android:layout_width="28dp"
            android:layout_height="74dp" />

        <Button
            android:id="@+id/btnOp3"
            android:layout_width="wrap_content"
            android:layout_height="72dp"
            android:text="Button" />
    </TableRow>
</androidx.constraintlayout.widget.ConstraintLayout>